algoritmo "semnome"
// Função : Temperatura
// Autor : Evolua Sumare
// Data : 26/03/2020
var
      grauC, grauF: real
inicio
      Escreva("Quantos graus tem hoje? Celsius = ")
      leia(grauC)
      grauF <- (grauC * 1.8) + 32
      Escreva("Graus em Fahrenheit é = ", grauF)
fimalgoritmo
