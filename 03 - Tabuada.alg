algoritmo "tabuada"
// Função : Tabuada
// Autor : Evolua Sumare
// Data : 26/03/2020
var
      cont, N, R: inteiro
inicio
      cont <- 1
      escreva (" Qual a tabuada deseja saber? ")
      leia (n)
      repita
        R <- N * Cont
        escreval (n,  " x ", cont, " = ", R)
        cont <- cont + 1
      ate (cont > 10)
fimalgoritmo
